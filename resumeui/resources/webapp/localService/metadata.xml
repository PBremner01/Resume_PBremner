<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<edmx:Edmx Version="1.0"
    xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx">
    <edmx:DataServices m:DataServiceVersion="2.0"
        xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
        <Schema Namespace="default"
            xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices"
            xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
            xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
            <EntityType Name="SubjectType">
                <Key>
                    <PropertyRef Name="sub_area"/>
                </Key>
                <Property Name="sub_area" Type="Edm.String" Nullable="false" MaxLength="100"/>
                <NavigationProperty Name="Subjects" Relationship="default.SubjectsType" FromRole="SubjectPrincipal" ToRole="AreaDependent"/>
            </EntityType>
            <EntityType Name="AreaType">
                <Key>
                    <PropertyRef Name="name"/>
                </Key>
                <Property Name="name" Type="Edm.String" Nullable="false" MaxLength="100"/>
                <Property Name="partof.sub_area" Type="Edm.String" MaxLength="100"/>
            </EntityType>
            <Association Name="SubjectsType">
                <End Type="default.SubjectType" Role="SubjectPrincipal" Multiplicity="1"/>
                <End Type="default.AreaType" Role="AreaDependent" Multiplicity="*"/>
            </Association>
            <EntityContainer Name="v2" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Subject" EntityType="default.SubjectType"/>
                <EntitySet Name="Area" EntityType="default.AreaType"/>
                <AssociationSet Name="Subjects" Association="default.SubjectsType">
                    <End Role="SubjectPrincipal" EntitySet="Subject"/>
                    <End Role="AreaDependent" EntitySet="Area"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
